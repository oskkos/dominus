{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"Apartment": {
				"description": "Apartment details",
				"properties": {
					"apartmentDescription": {
						"type": "string"
					},
					"roomCount": {
						"type": "integer",
						"format": "int32"
					},
					"surfaceArea": {
						"type": "number",
						"format": "double"
					},
					"streetAddress": {
						"type": "string"
					},
					"postalCode": {
						"type": "string"
					},
					"postDistrict": {
						"type": "string"
					}
				},
				"required": [
					"apartmentDescription",
					"roomCount",
					"surfaceArea",
					"streetAddress",
					"postalCode",
					"postDistrict"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"apartmentDescription": "2h+k+ph+parv.",
					"roomCount": 2,
					"surfaceArea": 54.5,
					"streetAddress": "Mallikatu 27 A 8",
					"postalCode": "33100",
					"postDistrict": "Tampere"
				}
			},
			"AuthToken": {
				"description": "AuthToken is used for granting access into dominus and identifying the user",
				"properties": {
					"id": {
						"type": "integer",
						"format": "int32"
					},
					"username": {
						"type": "string"
					},
					"accessToken": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"username",
					"accessToken"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"id": 82,
					"username": "oskkos",
					"accessToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6NCwidXNlcm5hbWUiOiJvc2trb3MiLCJpYXQiOjE1OTAyMjMxOTgsImV4cCI6MTU5MDMwOTU5OH0.tP-5h7NmH9aafoIKACrlkFZkLpY3slRRdJbTMk8jMHw"
				}
			},
			"AuthUser": {
				"description": "AuthUser is used for authenticating into dominus",
				"properties": {
					"username": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				},
				"required": [
					"username",
					"password"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"username": "oskkos",
					"password": "s3cret"
				}
			},
			"User": {
				"description": "Describes a user in a dominus",
				"properties": {
					"id": {
						"type": "integer",
						"format": "int32"
					},
					"username": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"username"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"id": 82,
					"username": "oskkos"
				}
			},
			"AddUserBody": {
				"description": "Payload for adding a new user",
				"properties": {
					"username": {
						"type": "string"
					},
					"password": {
						"type": "string"
					},
					"name": {
						"type": "string"
					}
				},
				"required": [
					"username",
					"password",
					"name"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"username": "oskkos",
					"password": "s3cret",
					"name": "Oskari Kosonen"
				}
			},
			"ChangePasswordBody": {
				"description": "Payload for changing password",
				"properties": {
					"oldPwd": {
						"type": "string"
					},
					"newPwd": {
						"type": "string"
					}
				},
				"required": [
					"oldPwd",
					"newPwd"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"oldPwd": "too_easy",
					"newPwd": "s3cret_p455word"
				}
			}
		},
		"securitySchemes": {
			"apiKey": {
				"type": "apiKey",
				"in": "header",
				"name": "x-access-token"
			}
		}
	},
	"info": {
