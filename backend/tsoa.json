{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers/*.controller.ts"],
  "spec": {
    "outputDirectory": ".",
    "specVersion": 3,
    "securityDefinitions": {
      "apiKey": {
        "type": "apiKey",
        "in": "header",
        "name": "x-access-token"
      }
    }
  },
  "routes": {
    "authenticationModule": "src/middlewares/auth.jwt.ts",
    "routesDir": "."
  }
}
